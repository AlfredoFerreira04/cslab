  <Requirements:Requirement xmi:id="_k9tWENU6EfCyXKz_9GZJUQ" base_NamedElement="_k9moYNU6EfCyXKz_9GZJUQ" id="R-1" text="While the system is in Automatic Mode, the ACCS shall adjust the smart blinds to the open position to maximize natural sunlight entry.&#xD;&#xA;" base_Class="_k9moYNU6EfCyXKz_9GZJUQ"/>
  <Requirements:Requirement xmi:id="_lcC34NU6EfCyXKz_9GZJUQ" base_NamedElement="_lb_0kNU6EfCyXKz_9GZJUQ" id="R-2" text="When the natural light level, at it's maximum, is measured below the user-&#xD;&#xA;" base_Class="_lb_0kNU6EfCyXKz_9GZJUQ"/>
  <Requirements:Requirement xmi:id="_lztJ8NU6EfCyXKz_9GZJUQ" base_NamedElement="_lzqGoNU6EfCyXKz_9GZJUQ" id="R-3" text="While the system is in Automatic Mode, the ACCS shall modulate the smart heater output to maintain the office temperature within user preferences.&#xD;&#xA;" base_Class="_lzqGoNU6EfCyXKz_9GZJUQ"/>
  <Requirements:Requirement xmi:id="_mJUNkNU6EfCyXKz_9GZJUQ" base_NamedElement="_mJRxUNU6EfCyXKz_9GZJUQ" id="R-4" text="When the Occupant inputs configuration changes via the User Interface, the ACCS shall store the new preferences for lighting and temperature." base_Class="_mJRxUNU6EfCyXKz_9GZJUQ"/>
  <Requirements:Requirement xmi:id="_mgtZ4NU6EfCyXKz_9GZJUQ" base_NamedElement="_mgrksNU6EfCyXKz_9GZJUQ" id="R-5" text="When a manual command is received for a specific device (Blinds/Lights/Heater), the ACCS shall override the automatic control and execute the requested status change.&#xD;&#xA;" base_Class="_mgrksNU6EfCyXKz_9GZJUQ"/>
  <Requirements:Requirement xmi:id="_m299wNU6EfCyXKz_9GZJUQ" base_NamedElement="_m28IkNU6EfCyXKz_9GZJUQ" id="R-6" text="If the measured environmental parameters deviate from the current system configuration target values for a defined duration, the ACCS shall display a warning message on the User Interface. " base_Class="_m28IkNU6EfCyXKz_9GZJUQ"/>
